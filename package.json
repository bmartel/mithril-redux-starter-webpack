{
  "name": "mithril-redux-starter-webpack",
  "version": "1.0.0",
  "description": "Mithril Redux + Webpack",
  "license": "MIT",
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "watch": "NODE_ENV=development webpack-dev-server --content-base ./dist/ --inline --hot --host=0.0.0.0 --port=9000",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "prebuild": "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "test": "mocha --compilers css:./test-compiler,js:babel-core/register --recursive",
    "lint": "eslint ./src ./test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "lodash": "^4.17.11",
    "midux": "^2.1.1",
    "mithril": "^1.1.6",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "image-webpack-loader": "^4.4.0",
    "imports-loader": "^0.8.0",
    "jsdom": "^12.2.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mithril-query": "^2.5.0",
    "mocha": "^5.2.0",
    "postcss": "^7.0.5",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "preload-webpack-plugin": "^2.3.0",
    "prettier": "1.14.3",
    "prettier-eslint": "^8.8.2",
    "purgecss-webpack-plugin": "^1.3.1",
    "rimraf": "^2.6.1",
    "sinon": "^7.0.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^0.6.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
