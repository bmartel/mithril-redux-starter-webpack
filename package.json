{
  "name": "mithril-redux-starter-webpack",
  "version": "1.0.0",
  "description": "Mithril Redux + Webpack",
  "license": "MIT",
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "watch":
      "NODE_ENV=development webpack-dev-server --content-base ./dist/ --inline --hot --host=0.0.0.0 --port=9000",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "prebuild": "npm run clean",
    "clean":
      "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "test":
      "mocha --compilers css:./test-compiler,js:babel-core/register --recursive",
    "lint": "eslint ./src ./test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "lodash": "^4.17.4",
    "midux": "^2.0.0",
    "mithril": "^1.1.3",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "autoprefixer": "^8.2.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.3.1",
    "imports-loader": "^0.7.1",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.0.4",
    "mithril-query": "^2.2.0",
    "mocha": "^3.5.0",
    "postcss": "^6.0.9",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-url": "^7.3.2",
    "prettier": "1.12.0",
    "prettier-eslint": "^8.8.1",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.0",
    "style-loader": "^0.18.2",
    "tailwindcss": "^0.5.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "browserslist": ["> 2%", "last 2 versions", "not ie <= 9"]
}
