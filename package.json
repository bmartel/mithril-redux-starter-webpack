{
  "name": "mithril-redux-starter-webpack",
  "version": "1.0.0",
  "description": "Mithril Redux + Webpack",
  "license": "MIT",
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "watch": "NODE_ENV=development webpack-dev-server --content-base ./dist/ --inline --hot --host=0.0.0.0 --port=9000",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "prebuild": "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "test": "mocha --compilers css:./test-compiler,js:babel-core/register --recursive",
    "lint": "eslint ./src ./test"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "lost": "^8.2.0",
    "midux": "^2.0.0",
    "mithril": "^1.1.3",
    "normalize.css": "^7.0.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.1",
    "css-loader": "^0.28.4",
    "es6-promise": "^4.1.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "image-webpack-loader": "^3.3.1",
    "imports-loader": "^0.7.1",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "mithril-query": "^2.2.0",
    "mocha": "^3.5.0",
    "postcss": "^6.0.9",
    "postcss-cssnext": "^3.0.2",
    "postcss-extend": "^1.0.5",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "rucksack-css": "^1.0.2",
    "sinon": "^3.2.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "engines": {
    "node": ">=7.6"
  }
}
